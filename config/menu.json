{  	
    "headerParams": {
        "darkMode": true,
        "logo": "logo-studency.jpg",
        "logoHeight": "50"
    },

    "applications": {
		"marketing": {
			"labels": { "default": "Marketing" },
			"default": "automationTemplate"
        },
        "admission": {
			"labels": { "default": "Admission" },
			"default": "prospect",
			"defaultRole": "sales_manager"
        },
        "apprentissage": {
			"labels": { "default": "Apprentissage" },
			"default": "appEvaluation"
        },
		"finance": {
			"labels": { "default": "Flow-ER Finance" },
			"default": "fiCommitment",
			"defaultRole": "sales_manager"
        },
		"learning": {
			"labels": { "default": "Flow-ER Learning" },
			"default": "learningLearner",
			"defaultRole": "manager"
        },
		"bi": {
			"labels": { "default": "Flow-ER BI" }
        },
		"admin": {
			"labels": { "default": "Admin" },
			"default": "profile"
        },
		"studio": {
			"labels": { "default": "Flow-ER Studio" }
        }
    },

    "menu/admission": {
        "tabs": ["tab/prospect", "tab/commercial", "tab/event", "tab/configView"],
        "defaultTab": "tab/prospect",
        "labels": { "default": "Admission" }
    },

    "tab/prospect": {
        "menu": "menu/admission",
        "controller": "mdb",
        "action": "index",
        "entity": "candidat",
        "view": "prospect",
        "labels": { "default": "Prospects" }
    },

    "tab/commercial": {
        "menu": "menu/admission",
        "controller": "mdb",
        "action": "index",
        "entity": "candidat",
        "view": "commercial",
        "labels": { "default": "Candidats" }
    },

    "tab/event": {
        "menu": "menu/admission",
        "controller": "mdb",
        "action": "index",
        "entity": "event",
        "view": "event",
        "labels": { "default": "Calendrier" }
    },

    "tab/configView": {
        "menu": "menu/admission",
        "controller": "bo",
        "action": "index",
        "entity": "config",
        "view": "confgiView",
        "labels": { "default": "Views", "fr_FR": "Vues" }
    },

    "tab/auditCandidat": {
        "menu": "menu/admin",
        "controller": "bo",
        "action": "index",
        "entity": "audit",
        "view": "auditCandidat",
        "labels": { "default": "Audit candidats" }
    },

    "menu/bi": {
        "tabs": ["tab/auditStatus", "tab/auditOrigine"],
        "defaultTab": "tab/auditStatus",
        "labels": { "default": "BI" }
    },

    "tab/auditStatus": {
        "menu": "menu/bi",
        "controller": "mdb",
        "action": "index",
        "entity": "audit",
        "view": "auditStatus",
        "labels": { "default": "Actions commerciales 2024" }
    },

    "tab/auditOrigine": {
        "menu": "menu/bi",
        "controller": "mdb",
        "action": "index",
        "entity": "audit",
        "view": "auditOrigine",
        "route": "/mdb/index/audit",
        "urlParams": "view=auditOrigine",
        "labels": { "default": "Suivi par origine (à faire)" }
    },

    "menu/marketing": {
        "tabs": ["tab/automationTemplate", "tab/campaign"],
        "defaultTab": "tab/automationTemplate",
        "labels": { "default": "Marketing" }
    },

    "tab/automationTemplate": {
        "menu": "menu/marketing",
        "controller": "mdb",
        "action": "index",
        "entity": "document_automation_template",
        "view": "automationTemplate",
        "labels": { "default": "Automation templates" }
    },

    "tab/campaign": {
        "menu": "menu/marketing",
        "controller": "mdb",
        "action": "index",
        "entity": "mkt_targeted",
        "view": "campaign",
        "labels": { "default": "Campagnes" }
    },

    "menu/apprentissage": {
        "tabs": ["tab/appEntreprise", "tab/appEvaluation"],
        "defaultTab": "tab/appEntreprise",
        "labels": { "default": "Apprentissage" }
    },

    "tab/appEntreprise": {
        "menu": "menu/apprentissage",
        "controller": "mdb",
        "action": "index",
        "entity": "entreprise",
        "view": "appEntreprise",
        "labels": { "default": "Entreprises" }
    },

    "tab/appEvaluation": {
        "menu": "menu/apprentissage",
        "controller": "mdb",
        "action": "index",
        "entity": "document_apprentissage",
        "view": "appEvaluation",
        "labels": { "default": "Évaluations tuteurs" }
